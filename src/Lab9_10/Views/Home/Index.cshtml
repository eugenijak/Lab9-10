@model IEnumerable<Lab9_10.Models.Human>
@{
    ViewBag.Title = "All human info";
}
<a asp-action="Create">Add human</a>


<table class="table">
    <tr><td>Имя</td><td>Номер</td></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Phone</td>
            <td>
                <a asp-action="Edit" asp-controller="Home" asp-route-id="@item.Id">Изменить</a> |
                <a asp-action="Delete" asp-controller="Home" asp-route-id="@item.Id">Удалить</a>
            </td>
        </tr>
    }
</table>

